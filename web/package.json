{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "24.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run lint:design || true",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings=1000",
    "lint:design": "design-lint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:staging": "playwright test",
    "test:staging:ui": "playwright test --ui",
    "test:staging:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:staging"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@sentry/nextjs": "^10.27.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.85.0",
    "ajv": "^8.17.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "googleapis": "^166.0.0",
    "isomorphic-fetch": "^3.0.0",
    "next": "16.0.10",
    "openai": "^6.9.1",
    "openid-client": "^6.8.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.5.1",
    "resend": "^6.5.2",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@lapidist/design-lint": "^6.0.6",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.16"
  }
}
